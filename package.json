{
  "name": "vue-material-dashboard-pro",
  "version": "1.0.1",
  "description": "Vue Material Dashboard PRO",
  "author": "Dragos Ct <dragos@creative-tim.com>",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "dev": "npm run serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test --watch",
    "lint": "vue-cli-service lint",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run dev"
  },
  "dependencies": {
    "@tweenjs/tween.js": "16.6.0",
    "chartist": "0.11.0",
    "es6-promise": "4.2.4",
    "fullcalendar": "3.9.0",
    "fuse.js": "3.2.1",
    "google-maps": "3.3.0",
    "jquery": "3.3.1",
    "jvectormap-next": "3.0.0",
    "nouislider": "11.1.0",
    "npm": "6.0.1",
    "perfect-scrollbar": "1.4.0",
    "sweetalert2": "7.26.12",
    "vee-validate": "2.1.0-beta.8",
    "vue": "2.5.17",
    "vue-clickaway": "2.2.2",
    "vue-material": "1.0.0-beta-10.2",
    "vue-router": "3.0.1",
    "vue2-transitions": "0.2.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.0.1",
    "@vue/cli-plugin-eslint": "3.0.1",
    "@vue/cli-plugin-unit-mocha": "3.0.1",
    "@vue/cli-service": "3.0.1",
    "@vue/test-utils": "1.0.0-beta.24",
    "babel-plugin-component": "1.1.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "chai": "4.1.2",
    "lint-staged": "7.2.2",
    "node-sass": "4.9.3",
    "sass-loader": "7.1.0",
    "vue-template-compiler": "2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
